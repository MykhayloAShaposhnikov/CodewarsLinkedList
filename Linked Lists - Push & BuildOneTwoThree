from preloaded import Node

'''
Node is defined in preloaded like this:

class Node:
    def __init__(self, data):
        self.data = data
        self.next = None
'''
    
def push(head, data):
    res = Node(data)
    res.next = head
    return res

def build_one_two_three():
    xd = None
    xd = push(xd, 3)
    xd = push(xd, 2)
    xd = push(xd, 1)
    return xd
